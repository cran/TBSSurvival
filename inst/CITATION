citHeader("To cite TBSSurvival in publications please use:")

citEntry(entry = "Article",
  title        = "Transform Both Sides Model: A Parametric Approach",
  author       = personList(person("Adriano", "Polpo"),
                   as.person("Adriano Polpo"),
				   as.person("Cassio P. de Campos"),
                   as.person("Debajyoti Sinha"),
				   as.person("Stuart Lipsitz"),
				   as.person("Jianchang Lin")),
  journal      = "Under review",
  year         = "2012",
  url          = "http://code.google.com/p/tbssurvival/",
  textVersion  =
  paste("A. Polpo and C. P. de Campos and D. Sinha and S. Lipsitz and J. Lin (2012).",
        "TBSSurvival R package",
        "URL http://code.google.com/p/tbssurvival/.")
)
