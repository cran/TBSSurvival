  url          = "http://code.google.com/p/tbssurvival/",
  textVersion  =
  paste("A. Polpo and C. P. de Campos and D. Sinha and S. Lipsitz and J. Lin (2014).",
        "Transform Both Sides Model: A Parametric Approach",
        "Computational Statistics and Data Analysis http://dx.doi.org/10.1016/j.csda.2013.07.023")
